{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "sugarcube",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "item"
      }
    },
    "item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "naked_variable"
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "SYMBOL",
          "name": "text"
        }
      ]
    },
    "comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "<!--"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "PATTERN",
                "value": "[^-]"
              },
              {
                "type": "PATTERN",
                "value": "-[^-]"
              },
              {
                "type": "PATTERN",
                "value": "--[^>]"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "-->"
        }
      ]
    },
    "line_continuation": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\\"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SEQ",
                "members": [
                  {
                    "type": "CHOICE",
                    "members": [
                      {
                        "type": "STRING",
                        "value": "\r"
                      },
                      {
                        "type": "BLANK"
                      }
                    ]
                  },
                  {
                    "type": "STRING",
                    "value": "\n"
                  }
                ]
              },
              {
                "type": "STRING",
                "value": "\u0000"
              }
            ]
          }
        ]
      }
    },
    "text": {
      "type": "TOKEN",
      "content": {
        "type": "PREC",
        "value": -1,
        "content": {
          "type": "PATTERN",
          "value": "[^$][a-zA-Z0-9\\]\\[ :.]*"
        }
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-z][a-zA-Z0-9]*"
    },
    "naked_variable": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "$"
        },
        {
          "type": "PATTERN",
          "value": "[a-z][a-zA-Z0-9]*"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "dotProperty"
              },
              {
                "type": "SYMBOL",
                "name": "squareProperty"
              }
            ]
          }
        }
      ]
    },
    "dotProperty": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "."
        },
        {
          "type": "PATTERN",
          "value": "[a-z][a-zA-Z0-9]*"
        }
      ]
    },
    "squareProperty": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "SYMBOL",
          "name": "expression"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "\""
              },
              {
                "type": "PATTERN",
                "value": "[^$][a-zA-Z0-9\\]\\[ :.]*"
              },
              {
                "type": "STRING",
                "value": "\""
              }
            ]
          }
        },
        {
          "type": "TOKEN",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "STRING",
                "value": "'"
              },
              {
                "type": "PATTERN",
                "value": "[^$][a-zA-Z0-9\\]\\[ :.]*"
              },
              {
                "type": "STRING",
                "value": "'"
              }
            ]
          }
        }
      ]
    },
    "expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PREC_LEFT",
          "value": 0,
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "expression"
              },
              {
                "type": "CHOICE",
                "members": [
                  {
                    "type": "STRING",
                    "value": "+"
                  },
                  {
                    "type": "STRING",
                    "value": "-"
                  }
                ]
              },
              {
                "type": "SYMBOL",
                "name": "expression"
              }
            ]
          }
        },
        {
          "type": "PREC",
          "value": 5,
          "content": {
            "type": "PREC_LEFT",
            "value": 0,
            "content": {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "expression"
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": "*"
                    },
                    {
                      "type": "STRING",
                      "value": "/"
                    }
                  ]
                },
                {
                  "type": "SYMBOL",
                  "name": "expression"
                }
              ]
            }
          }
        },
        {
          "type": "SYMBOL",
          "name": "naked_variable"
        },
        {
          "type": "SYMBOL",
          "name": "literal"
        }
      ]
    },
    "literal": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    },
    {
      "type": "PATTERN",
      "value": "[\\s\\f\\uFEFF\\u2060\\u200B]|\\r?\\n"
    },
    {
      "type": "SYMBOL",
      "name": "line_continuation"
    }
  ],
  "conflicts": [
    [
      "expression",
      "literal"
    ],
    [
      "item",
      "expression",
      "literal"
    ],
    [
      "item",
      "expression"
    ]
  ],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}
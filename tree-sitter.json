{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "sugarcube",
      "camelcase": "SugarCube",
      "title": "SugarCube",
      "scope": "source.sugarcube",
      "file-types": [
        "twee"
      ],
      "injection-regex": "^sugarcube$",
      "class-name": "TreeSitterSugarcube"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter parser for the SugarCube format of Twine",
    "authors": [
      {
        "name": "Antoine Omond",
        "email": "antoine.omond@ntymail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-sugarcube"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}